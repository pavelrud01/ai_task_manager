{
  "$schema":"http://json-schema.org/draft-07/schema#",
  "type":"object",
  "required":["algo","features","clusters"],
  "properties":{
    "algo":{"type":"string"},
    "features":{"type":"array","items":{"type":"string"}},
    "clusters":{"type":"array","minItems":1,"items":{
      "type":"object","required":["id","label","members","unify_rule"],
      "properties":{
        "id":{"type":"string"},
        "label":{"type":"string"},
        "members":{"type":"array","minItems":1,"items":{"type":"string"}},
        "unify_rule":{"type":"string"}
      }
    }}
  }
}
